<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="watcherConfig" type="RoyaltyWorker.Config.WatcherConfigSection, RoyaltyWorker" />
    <section name="workerConfig" type="RoyaltyWorker.Config.WorkerConfigSection, RoyaltyWorker" />
  </configSections>
  <watcherConfig timerInterval="00:00:20.000"
                 exceptionIfNoOneFileInQueue="false"
                 verboseLog="false"/>
  <workerConfig timerInterval="00:00:05.000"
                logFileEncodingName="utf-8"
                verboseLog="false"
                defaultImportLogFileName="fileimport.log"
                defaultExportFileName="export.csv"
                exportFileEncodingName="utf-8"
                defaultNotTrustedPhonesSuffix="phones"
                defaultCultureForExportThread="ru-RU"/>
</configuration>